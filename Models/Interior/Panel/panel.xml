<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ASI -->
  <model>
    <path>Aircraft/MS-406/Models/Interior/Panel/Instruments/airspeed/airspeed.xml</path>
    <offsets>
      <x-m> -0.069 </x-m>
      <y-m>  0.062 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

  <!-- ALT -->
  <model>
    <path>Aircraft/MS-406/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m> -0.069 </x-m>
      <y-m>  0.083 </y-m>
      <z-m>  0.078 </z-m>
    </offsets>
  </model>

  <!-- VSI -->
  <model>
    <path>Aircraft/MS-406/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m> -0.069 </x-m>
      <y-m> -0.062 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

</PropertyList>
